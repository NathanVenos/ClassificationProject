    "\n",
    "guess_TP = 0\n",
    "guess_TN = len(y_test_under[y_test_under==0])\n",
    "guess_FP = 0\n",
    "guess_FN = len(y_test_under[y_test_under==1])\n",
    "\n",
    "print(\"\")\n",
    "print(\"Random Guess Accuracy: {:.4}%\".format( \n",
    "    (guess_TP + guess_TN) / (guess_TP + guess_TN + guess_FP + guess_FN) * 100))"
